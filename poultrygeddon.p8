version 43
__lua__
function _init()
	plr=plr_new(60,60)
	cam={x=0,y=0}
end

function _update60()
	plr_update(plr)
	cam_follow(cam,plr)
end

function _draw()
	cls()

	map()
	
	camera(cam.x,cam.y)
	
	plr_draw(plr)
end

pico-8 cartridge // http://www.pico-8.com
version 43
__lua__
function _init()
	plr=plr_new(map_w/2,map_h/2)
	cam={
		x=plr.x-(screen_size/2),
		y=plr.y-(screen_size/2),
	}
end

function _update60()
	plr_update(plr)
	cam_follow(cam,plr)
end

function _draw()
	cls()

	map()
	
	-- draw grass
	if not grass_drawn then
		for x=0,map_w/8 do
			for y=0,map_h/8 do
				local n=mget(x,y)
				local r=rnd(100)
				local sprs={2,3,4}
				if r<10 and n==1 then
					mset(x,y,rnd(sprs))
				end
			end
		end
		grass_drawn=true
	end
	
	camera(cam.x,cam.y)
	
	plr_draw(plr)
end
